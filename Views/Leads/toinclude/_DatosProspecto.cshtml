<div class="row mb-4">
    <h3>Tendencia del Prospecto</h3>
    <div class="col-md-6">
        <div class="form-group">
            <label for="PROPENSION">Propensión</label>
            <input class="form-control form-control-editable" id="PROPENSION" type="text" value="Propension"
                readonly>
        </div>
        <div class="form-group">
            <label for="TIPO_CLIENTE">Tipo de Cliente</label>
            <input class="form-control form-control-editable" id="TIPO_CLIENTE" type="text" value="TipoCliente"
                readonly>
        </div>
        <div class="form-group">
            <label for="CLIENTE_NUEVO">Antiguedad del Cliente</label>
            <input class="form-control form-control-editable" id="CLIENTE_NUEVO" type="text" value="ClienteNuevo"
                readonly>
        </div>
    </div>
    <div class="col-md-6">
        <!-- Colores -->
        <div class="form-group">
            <label for="COLOR">Color Principal</label>
            <div class="d-flex align-items-center">
                <input class="form-control ml-2"
                    style="width: 10%;" readonly>
                <input class="form-control ml-2" id="COLOR" type="text" value="Color" readonly>
            </div>
        </div>
        <div class="form-group">
            <label for="COLOR_FINAL">Color Final</label>
            <div class="d-flex align-items-center">
                <input class="form-control ml-2"
                    style="width: 10%;"
                    readonly>
                <input class="form-control ml-2" id="COLOR_FINAL" type="text" value="ColorFinal" readonly>
            </div>
        </div>
        <div class="form-group">
            <label for="TIPO_USUARIO">Usuario Tipo</label>
            <input class="form-control form-control-editable" id="TIPO_USUARIO" type="text" value="Usuario"
                readonly>
